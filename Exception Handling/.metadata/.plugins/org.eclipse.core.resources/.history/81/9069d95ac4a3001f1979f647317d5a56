package com.exception;
class Hai{
	void m1(int i) {
		System.out.println("m1 in hai");
		if(i==0) {
			throw new ArrayIndexOutOfBoundsException();
		}else {
			throw new ArithmeticException();
		}
	}
}


public class CheckedUnchecked {
	public static void main(String[] args) {
		Hai h1=new Hai();
		h1.m1(1);
	}
}
